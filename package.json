{
  "name": "xiaopang-cli",
  "version": "1.0.1",
  "description": "奔跑的小胖个人脚手架",
  "main": "index.js",
  "bin": {
    "xiaopang": "./bin/xiaopang.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged && npm run test",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chelseachen007/xiaopang-Cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chelseachen007/xiaopang-Cli/issues"
  },
  "homepage": "https://github.com/chelseachen007/xiaopang-Cli#readme",
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "commitmsg && npm run precommit"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "child_process": "^1.0.2",
    "clear": "^0.1.0",
    "commander": "^6.0.0",
    "download-git-repo": "^3.0.2",
    "figlet": "^1.5.0",
    "handlebars": "^4.7.6",
    "open": "^7.1.0",
    "ora": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5"
  }
